[CreateBG]=00:10:17	BGS	Event03/03-SA/03-SA-D01/03-SA-D01-002B	00:15:14;

[CreateBG]=00:15:14	BGS	Event03/03-SA/03-SA-D01/03-SA-D01-002A	00:20:03;

