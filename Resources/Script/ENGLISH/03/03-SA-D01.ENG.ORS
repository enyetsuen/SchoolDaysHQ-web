[CreateBG]=00:55:07	BGS	Event03/03-SA/03-SA-D01/03-SA-D01-007	01:05:12;

[PrintText]=00:58:16	Makoto	Sure... Kioura's going to kill me if she finds out, isn't she?	01:02:21;

[PlayVoice]=00:58:16	Voice03/03-SA/03-SA-D01/03-SA-D01-0220	1	mak	01:02:21;

[PrintText]=01:02:21	Sekai	Ahaha, that's true.	01:05:11;

[PlayVoice]=01:02:21	Voice03/03-SA/03-SA-D01/03-SA-D01-0230	0	sek	01:05:11;

[PrintText]=01:05:11	Makoto	Now, then... where should we go?	01:08:00;

