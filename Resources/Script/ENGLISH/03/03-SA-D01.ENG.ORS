[PlayVoice]=00:31:20	Voice03/03-SA/03-SA-D01/03-SA-D01-0140	1	mak	00:34:00;

[PrintText]=00:33:23	Sekai	Ah! Hey, listen! I was the one who made the lemon custard!	00:38:13;

[PlayVoice]=00:33:23	Voice03/03-SA/03-SA-D01/03-SA-D01-0150	0	sek	00:38:13;

[CreateBG]=00:34:00	BGS	Event03/03-SA/03-SA-D01/03-SA-D01-005	00:46:00;

