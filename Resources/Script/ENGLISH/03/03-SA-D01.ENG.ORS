[CreateBG]=00:46:00	BGS	Event03/03-SA/03-SA-D01/03-SA-D01-006	00:55:07;

[PrintText]=00:48:06	Sekai	Actually, it's a sweets shop, but they're famous for their cakes.	00:51:20;

[PlayVoice]=00:48:06	Voice03/03-SA/03-SA-D01/03-SA-D01-0190	0	sek	00:51:20;

[PrintText]=00:51:20	Makoto	I see. You got proper training, then.	00:55:07;

[PlayVoice]=00:51:20	Voice03/03-SA/03-SA-D01/03-SA-D01-0200	1	mak	00:55:07;

[PrintText]=00:55:06	Sekai	Uh-huh. So go ahead and eat plenty of it.	00:58:15;

