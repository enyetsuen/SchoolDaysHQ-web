[PrintText]=00:26:12	Kotonoha	Piano, ballet, dance, calligraphy... all sorts of things.	00:34:13;

[PlayVoice]=00:26:12	Voice05/05-SC/05-SC-E00/05-SC-E00-0110	0	kot	00:34:13;

